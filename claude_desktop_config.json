{
  "mcpServers": {
    "context7": {
      "command": "node",
      "args": [
        "D:\\New folder\\dashboarddesignanalysis\\node_modules\\@upstash\\context7-mcp\\dist\\index.js",
        "--api-key",
        "YOUR_UPSTASH_API_KEY"
      ],
      "env": {}
    },
    "postgres": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-postgres",
        "postgresql://postgres:postgres@localhost:5432/lider_garant"
      ],
      "env": {}
    },
    "redis": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-redis",
        "redis://localhost:6379"
      ],
      "env": {}
    },
    "docker": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-server-docker"
      ],
      "env": {}
    },
    "github": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-github"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "YOUR_GITHUB_PERSONAL_ACCESS_TOKEN"
      }
    },
    "puppeteer": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-puppeteer"
      ],
      "env": {}
    },
    "memory": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ],
      "env": {}
    },
    "think": {
      "command": "npx",
      "args": [
        "-y",
        "think-mcp"
      ],
      "env": {}
    },
    "openapi": {
      "command": "npx",
      "args": [
        "-y",
        "openapi-mcp-server"
      ],
      "env": {}
    },
    "django-rest": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-remote",
        "http://localhost:8000/mcp/",
        "--transport",
        "http-only"
      ],
      "env": {}
    }
  }
}